{
  "expo": {
    "name": "FLO",
    "slug": "flo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/flo-icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/flo-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "android": {
      "package": "com.chiwbacca.floapp",
    "versionCode": 1,
    "adaptiveIcon": {
      "foregroundImage": "./assets/flo-icon.png",
      "backgroundColor": "#000000"
    },
    "permissions": [
      "android.permission.NFC",
      "android.permission.INTERNET",
      "android.permission.ACCESS_NETWORK_STATE"
    ],
    "intentFilters": [
      {
        "action": "android.nfc.action.NDEF_DISCOVERED",
        "data": [
          {
            "mimeType": "text/plain"
          }
        ]
      }
    ]
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "extra": {
    "eas": {
      "projectId": "b5a52564-87e8-47a5-b995-1bdb95e021d1"
    }
  },
  "owner": "chiwbacca"
}
